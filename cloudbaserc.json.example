{
  "envId": "your-env-id",
  "functionRoot": "functions",
  "functions": [
    {
      "name": "callBailian",
      "timeout": 60,
      "envVariables": {
        "DASHSCOPE_API_KEY": "请在此处填入您的阿里云百炼 API Key",
        "DOUBAO_API_KEY": "请在此处填入您的火山方舟豆包 API Key（用于豆包图生图功能）",
        "TENCENT_SECRET_ID": "请在此处填入您的腾讯云 SecretId（本地测试时需要）",
        "TENCENT_SECRET_KEY": "请在此处填入您的腾讯云 SecretKey（本地测试时需要）"
      },
      "runtime": "Nodejs16.13",
      "handler": "index.main",
      "memorySize": 512
    },
    {
      "name": "callRUMAPI",
      "timeout": 30,
      "envVariables": {
        "TENCENT_SECRET_ID": "请在此处填入您的腾讯云 SecretId（用于调用RUM API，推荐在控制台配置）",
        "TENCENT_SECRET_KEY": "请在此处填入您的腾讯云 SecretKey（用于调用RUM API，推荐在控制台配置）"
      },
      "runtime": "Nodejs16.13",
      "handler": "index.main",
      "memorySize": 256
    },
    {
      "name": "fusion",
      "timeout": 30,
      "envVariables": {
        "TENCENT_SECRET_ID": "请在此处填入您的腾讯云 SecretId（用于人脸融合API，推荐在控制台配置）",
        "TENCENT_SECRET_KEY": "请在此处填入您的腾讯云 SecretKey（用于人脸融合API，推荐在控制台配置）"
      },
      "runtime": "Nodejs16.13",
      "handler": "index.main",
      "memorySize": 256
    }
  ]
}

